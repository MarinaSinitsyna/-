#include <stdio.h>
	int main ()
	{
	int hour, min, sec;
	printf("how meny hors, minutes,seconds\n");
	scanf("%d",&hour);
	scanf("%d",&min);
	scanf("%d",&sec); 
	printf("%d:%d:%d\n",hour,min,sec);
	if (5<hour && hour<=9)
		printf("good morning\n");
	else if (hour>9 && hour<=16)
		printf("good afternoon\n");
	else if (hour>16 && hour<=23)
		printf("good evening\n");
	else if (hour>23 && hour<=5)
		printf("good night\n");
	else  
		printf("error");
	return 0;
	}
