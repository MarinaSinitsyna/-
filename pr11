#include <stdio.h>
int main ()
{
	int a;
	float b, k;
	char d;
	printf("enter your height  \n"); //рост
	scanf("%d",&a);
	printf("enter your weight\n");  //вес
	scanf("%f",&b);
	printf("enter your floor M or F\n");   //пол
	scanf("%s",&d);
	switch (d)
	{
	case 'M':
		 k = (a - 100)*0.85f;
	//default:
		break;
	case 'F':
		 k = (a - 100)*0.85f;
	break;
	}


return 0;
}
